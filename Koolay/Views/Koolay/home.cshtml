<!--================Get in Touch Area =================-->
<section id="iletisim" class="get_in_touch_area">
    <div class="container">
        <div class="row get_touch_inner " data-scroll-reveal="enter left move 40px over 0.6s after 0.3s">

            <div class="col-lg-8 col-md-8 col-xs-12">
                <div class="touch_details">
                    <div class="l_title">
                        <img src="~/img/icon/title-icon-2.png" alt="">
                        
                    </div>
                    <h4>Send us an e-mail </h4>
                    <p>
                        As KoolayBooks, we have 24/7 technical support service for our users, including during the end of the year and public holidays.
                    </p>
                </div>
            </div>
            <div class="col-lg-4 col-md-4 col-xs-12 align-self-center">
                <div class="contact_us_form " data-scroll-reveal="enter ease-in-out move 30px over 0.4s after 0.4s">
                    <button data-toggle="modal" style="cursor:pointer" type="button" data-target="#Modal1" class="more_btn submit_btn" id="btn-shake">Get in Touch</button>
                </div>
                @model Koolay.Models.Email
                <div class="modal" aria-labelledby="exampleModalCenterTitle" aria-hidden="true" tabindex="-1" id="Modal1" role="dialog">
                    <div class="vertical-alignment-helper">
                        <div class="modal-dialog modal-lg vertical-align-center">
                            <div class="modal-content">
                                    <button type="button" class="close" style="cursor:pointer; text-align:right; padding-right:7px" data-dismiss="modal">x</button>
                                
                                <div class="modal-body">
                                    <form class="contact_us_form row php-email-form" role="form" method="post" id="contactForm"
                                          novalidate="novalidate">
                                        <div class="form-group col-lg-6">
                                            <input type="text" class="form-control" id="ad" name="ad" placeholder="Name">
                                        </div>
                                        <div class="form-group col-lg-6">
                                            <input type="email" class="form-control" id="email" name="email" placeholder="Email">
                                        </div>
                                        <div class="form-group col-lg-12">
                                            <input type="text" class="form-control" id="konu" name="konu" placeholder="Subject*">
                                        </div>
                                        <div class="form-group col-lg-12">
                                            <textarea class="form-control" id="mesaj" name="mesaj" rows="1"
                                                      placeholder="Message"></textarea>
                                        </div>
                                        <div class="form-group col-lg-12">
                                            <button type="button" class="btn submit_btn form-control">Send »</button>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!--================End Get in Touch Area =================-->
